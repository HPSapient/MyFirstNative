{
  "expo": {
    "name": "MFN",
    "slug": "MyFirstNative",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Use loc in use",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "use loc in always",
        "NSLocationAlwaysUsageDescription": "use always",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "description": ""
  }
}